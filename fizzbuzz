n = 0
print("Input a number from 1 to 20. ")
while True:
    number = int(input(""))
    if number % 3 == 0 and number % 5 == 0:
        print("FizzBuzz")
    elif number % 3 == 0 and number % 5 != 0:
        print("Fizz")
    elif number % 3 != 0 and number % 5 == 0:
        print("Buzz")
    else:
        print(number)
    n = n + 1
    if n == 20:
        break
